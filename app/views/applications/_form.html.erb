<%= form_for (@application) do |f| %>
<%= f.label :desired_name %>
<%= f.text_field :title %>
<br>
<%= f.label :job %> <%= f.collection_select :job_id, Job.all, :id, :title, include_blank: true %>
<br>
<%= f.label :years_of_experience %>
<%= f.text_field :experience %>
<br>
<%= f.label :education %>
<%= f.text_field :description %>
<br>
<%= f.submit %>
<% end %>
<br>
<%= link_to "Account", user_path(current_user)%>
