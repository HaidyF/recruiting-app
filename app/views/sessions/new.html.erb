<h1>Sign In: </h1><br>

<%= form_for (@user) do |f| %>

  <%= f.label :username %><br>
  <%= f.text_field :username %><br>

   <%= f.label :email %><br>
  <%= f.text_field :email %><br>

  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <br>
  <%= f.submit 'Sign In' %>
  <% end %>